{
  "name": "sequential-migrate",
  "version": "1.0.0",
  "description": "Run scripts sequentially, and store a record of the scripts run",
  "bin": {
    "sequential-migrate": "build/index.js"
  },
  "scripts": {
    "dev": "npm run build && npm link && chmod +x build/index.js && sequential-migrate",
    "build": "rimraf ./build && tsc",
    "postbuild": "copyfiles -u 1 src/scaffold/* src/scaffold/**/* build",
    "docs": "typedoc src/types.ts",
    "test": "jest --watch"
  },
  "author": "Ivo Evans",
  "license": "MIT",
  "devDependencies": {
    "@types/jest": "^27.4.0",
    "@types/node": "^17.0.8",
    "@typescript-eslint/eslint-plugin": "^5.9.0",
    "@typescript-eslint/parser": "^5.9.0",
    "copyfiles": "^2.4.1",
    "dotenv": "^10.0.0",
    "eslint": "^8.6.0",
    "jest": "^27.4.7",
    "pg": "^8.7.1",
    "prettier": "^2.5.1",
    "rimraf": "^3.0.2",
    "ts-jest": "^27.1.2",
    "typedoc": "^0.22.10",
    "typedoc-plugin-markdown": "^3.11.9",
    "typescript": "^4.5.4"
  }
}
